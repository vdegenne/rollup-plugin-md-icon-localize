{
	"name": "rollup-plugin-md-icon-localize",
	"version": "0.0.5",
	"description": "Rollup plugin for localizing mw3 icons.",
	"main": "lib/index.js",
	"module": "lib/index.js",
	"types": "lib/index.d.ts",
	"type": "module",
	"author": "Valentin Degenne <vdegenne@gmail.com>",
	"license": "MIT",
	"bin": {
		"rpmil": "bin/rpmil.js"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/vdegenne/rollup-plugin-md-icon-localize.git"
	},
	"scripts": {
		"build": "wireit"
	},
	"wireit": {
		"build": {
			"command": "tsc --pretty",
			"clean": "if-file-deleted",
			"files": [
				"tsconfig.json",
				"src"
			],
			"output": [
				"lib"
			]
		}
	},
	"dependencies": {
		"@rollup/pluginutils": "^5.0.3",
		"fast-glob": "^3.3.1",
		"shelljs": "^0.8.5"
	},
	"devDependencies": {
		"@types/node": "^20.5.0",
		"@types/shelljs": "^0.8.12",
		"node-fetch": "^3.3.2",
		"rollup": "^3.28.0",
		"typescript": "^5.1.6",
		"wireit": "^0.10.0"
	},
	"files": [
		"/lib",
		"/bin",
		"logo.png"
	]
}
